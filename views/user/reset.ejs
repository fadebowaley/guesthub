      <main>
             
<section id="contact" class="contact-area after-none contact-bg pt-120 pb-120 p-relative fix">
  <div class="container">             
    <div class="row justify-content-center align-items-center">
      <div class="col-lg-8 order-1 offset-lg-2">
        <div class="contact-bg02">
          <div class="section-title center-align mb-40 text-center wow fadeInDown animated" data-animation="fadeInDown" data-delay=".4s">
            <h3>Change Password </h3>
                  <!-- Flash message -->
                   <% if (locals.errors) { %>
      <ul>
        <% locals.errors.forEach(function(error) { %>
          <li><%= error.msg %></li>
        <% }); %>
      </ul>
    <% } %>
 
          </div>  
      
          <form action="/user/reset/<%= token %>" method="POST" class="contact-form mt-30">
            <div class="row justify-content-center align-items-center">
              <div class="col-lg-8">
               <div class="contact-field p-relative c-message mb-30">                                  
                  <input type="password" id="passsword" name="password" placeholder="Password" required>
                </div>
                <div class="contact-field p-relative c-message mb-30">                                  
                  <input type="password" id="password2" name="password2" placeholder="Confirm Password" required>
                </div>                               
              </div>                                		
              <div class="col-lg-8">
            <input type="hidden" name="_csrf" value="<%= csrfToken %>" />                                    
                      
            <button class="btn ss-btn" data-animation="fadeInRight" data-delay=".8s" type="submit">
            <i class="fas fa-sign-in-alt"></i> Reset Password
            </button>                         
              </div>
            </div>
          </form>  
                          
          <div class="text-center mt-3">
            <span>Don't have account ? </span><a href="/user/register" style="color: rgb(239, 7, 7);"><strong>Sign Up</strong></a>
          </div>        
        </div>    
            
             
      </div>
    </div>
  
  </div> 
</section>

	
</main>
