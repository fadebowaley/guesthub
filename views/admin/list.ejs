<%- include ../partials/_head %>
<style>
.breadcrumb-area {
    background-size: cover;
    position: relative;
    min-height: 20px;
}

</style>
   <body>
        <!-- header -->
      <%- include ("../partials/_header") %>
        <!-- header-end -->
         <main>
         <!-- breadcrumb-area -->
            <section class="breadcrumb-area d-flex align-items-center" style="background-image:url/images/img/bg/bdrc-bg.jpg);">    <div class="container">
        <div class="row align-items-center">
            <div class="col-xl-12 col-lg-12">
                
            </div>
        </div>       
    </div>
</section>
<section id="service-details2" class="pt-120 pb-90 p-relative">
  <div class="animations-01"><img src="/images/img/bg/an-img-01.png" alt="an-img-01"></div>
  <div class="container">
    <div class="row align-items-center">
     
    <!-- container -->
      <div class="row">

     
        <div class="col-lg-12 col-md-6">            
          <div class="services-08-item mb-30">
            <div class="services-icon2">
              <img src="/images/img/icon/fe-icon04.png" alt="img">
            </div>
           
            


            <div class="services-08-content">
  <h3><a href="single-service.html">User Views</a></h3>
 
<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Status</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(function(user, index) { %>
      <tr>
        <th scope="row"><%= index + 1 %></th>
        <td><%= user.title %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.role %></td>
        <td>
          <button><i class="far fa-edit"></i> Edit</button>
          <button><i class="far fa-trash-alt"></i> Delete</button>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>


</div>



          </div>           
        </div> 
      </div>
  <!-- /container -->


    </div>
  </div>
</section>
</main>

  <%- include ("../partials/_footer") %>
